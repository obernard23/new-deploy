  <script>
    const updateCursor = ({ x, y }) => {
  document.documentElement.style.setProperty('--x', x)
  document.documentElement.style.setProperty('--y', y)
}


// universal serch
document.body.addEventListener('pointermove', updateCursor)

document.addEventListener('keydown', (event) => {
  var name = event.key;
  var code = event.code;
  // Alert the key name and key code on keydown
  if(code === "Space"){
    // alert(`Key pressed ${name} \r\n Key code value: ${code}`);
    document.getElementById('myModal').style.display ='block';

  }
}, false);

  // ai search
  const SEARCH = document.getElementById('SEARCH')
  SEARCH.addEventListener('change',async (e)=>{
   const request =  await fetch(`/api/v1/query/${e.target.value}`,{
    method:'GET',
    headers:{'Content-Type':'application/json'}
   })

    // get response
    const response = await request.json()

   if(response.message){
    document.getElementById('aiResponse').innerText = `Bade AI: ${response.message}`
   }else{
     alert(response.error)
   }
  })


  const BirthdayButtn = document.getElementById("birthdyButton")
  BirthdayButtn.addEventListener("click", async ()=>{
   const clientRequest =  await fetch('/api/v1/Birthday/messages',{
      method: 'GET',
      headers:{"content-type": "application/json"}
    })

    const serverResponse = await clientRequest.json()
    alert(serverResponse.message || serverResponse.error)
  });
  </script>
