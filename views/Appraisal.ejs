<!DOCTYPE html>
<html lang="en">
    <%-include('./partials/head.ejs')%>
    <%-include('./partials/inactive.ejs')%>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/Appraisal.css">
    <link rel="stylesheet" href="/main.css"/>
    <link rel="stylesheet" href="/feature.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <title>Appraisal</title>
</head>
<body>
    <nav class="navbar bg-black ">
        <div class="container-fluid">
          <a class="navbar-brand" href="/" title="Apps">
            <img
              src="data:image/png;base64,
                
                
                
                "
              width="30"
              height="30"
            />
          </a>
          <ul class="navbar-nav me-auto mb-2 mb-lg-0">
            <li class="nav-item dropdown">
              <h5>
                <a
                  class="nav-link breadcrumb-item active"
                  aria-current="page"
                  href="/"
                  role="button"
                  data-bs-toggle="dropdown"
                  aria-expanded="false"
                  style="color: #f0e9e9 !important"
                >
                  Appraisal
                </a>
              </h5>
            </li>
          </ul>
        </div>
      </nav>
   <article class="overflow-hidden">
    <!-- oprations show here -->
    <div  class="flex flex-col border mx-10 m-7 shadow-lg p-5 bg-light overflow-hidden" style="width:100vw;">
        <h1 class="font-bold text-2xl appraisalH1">Appraisal Form</h1>
        <!-- the main form div -->
        <div class="flex justify-between p-5">
            <div class="flex flex-col gap-3 ">
                <div class="input-group mb-3">
                    <span class="input-group-text" id="basic-addon1">Appraisee</span>
                    <input type="text" class="form-control" placeholder="Username" aria-label="Username" aria-describedby="basic-addon1" disabled value='<%=item.firstName%> <%=item.lastName%>'/>
                  </div>
                <!-- <input type="text" placeholder="Employee's name"  id="EmployeeName" style="width:570px;padding:10px;font-size: x-large;" required/> -->
                <div class="input-group mb-3">
                    <span class="input-group-text" id="basic-addon1">Manager</span>
                    <input type="text" class="form-control" placeholder="Username" aria-label="Username" disabled aria-describedby="basic-addon1">
                  </div>
                  <div class="input-group mb-3">
                    <span class="input-group-text" id="basic-addon1">Job Role</span>
                    <input type="text" class="form-control" value="<%=item.jobTittlew%>" placeholder="Username" aria-label="Username" disabled aria-describedby="basic-addon1">
                  </div>
                <div class="input-group mb-3">
                    <span class="input-group-text" id="basic-addon1">@</span>
                    <input type="text" class="form-control" placeholder="Username" aria-label="Username" disabled aria-describedby="basic-addon1" value="<%=item.Email%>">
                  </div>
                <label for="" class="flex justify-between text-sm font-bold">
                    Last Appraisal:<input type="text" id="lastAppraisal" disabled>
                </label>
            </div>
            <!-- for profile image -->
            <div style="width: 12rem;">
              <%if(item.image === ''){%>
                <img src=" https://cdn.pixabay.com/photo/2019/08/11/18/59/icon-4399701_640.png" alt="" srcset="">
                <%}else{%>
                  <img src="<%=item.image%>" class="card-img-top shadow-sm" width="140" height="140" alt="Photo" srcset="">
                <%}%>
            </div>
        </div>
        <div class="flex justify-center flex-col m-3 " >
            <h1 class="font-bold appraisalH1">Set KPI</h1>
        <!-- table format here -->
        <table>
            <thead>
            <th class=" border m-3">S/No</th>
            <th class="border">Perspective</th>
            <th class="border">Objectives</th>
            <th class="border">Measures</th>
            <th class=" border">Target</th>
            <th class=" border">Innovation</th>
        </thead>
        <tbody id="tbody"></tbody>
        <form >
            <tbody id="toggle" class=" justify-between">
                <tr>
                    <!-- submit form button -->
                    <td>
                        <button class=" font-bold border rounded p-1" id="submit">+</button>
                    </td>
                    <!-- Perspective -->
                    <td >
                        <div class="col-md-7">
                            <label for="Perspective" class="form-label"></label>
                            <select
                              class="form-select "
                              id="Perspective"
                              aria-describedby="validationServer04Feedback"
                              required
                              name="Perspective"
                            >
                            <option value="">Choose...</option>
                            <option value="Customer">Customer</option>
                            <option value="Finance">Finance</option>
                            <option value="Innovation">Innovation</option>
                            <option value="L&G">Learning and Growth</option>
                            </select>
                            </div>
                    </td>
                    <!-- Objectives -->
                    <td class="col-md-4">
                       
                            <label for="validationCustom01" class="form-label"></label>
                            <textarea type="text"  name="Objectives" cols="30" rows="2"  class="form-control" autocomplete="off" autocapitalize="on"  ></textarea>
                        
                    </td>
                    <!-- Measures -->
                    <td >
                        <div class="col-md-9">
                            <label for="Measures" class="form-label"></label>
                            <select
                              class="form-select "
                              id="Measures"
                              aria-describedby="validationServer04Feedback"
                              required
                              name="Measures"
                            >
                            <option value="">Choose...</option>
                            <option value="Q-1">[Q-1]</option>
                            <option value="Q-2">[Q-2]</option>
                            <option value="Q-3">[Q-3]</option>
                            <option value="Q-3">[Q-4]</option>
                            </select>
                            </div>
                    </td>
                    <!-- Target scoring -->
                    <td>
                        <label for="" class="flex justify-center">
                            <input type="number" name="score" placeholder="0" style="width: 50px;border:none" required/>%
                        </label>
                        
                    </td>
                    <!-- innovation -->
                    <td >
                        <label for="validationCustom01" class="form-label"></label>
                        <textarea type="text"  name="innovation" cols="30" rows="2"  class="form-control" autocomplete="off" autocapitalize="on"  ></textarea>
                    </td>
                </tr>
            </tbody>
        </table>
        </form>
    </div>
    <div class="p-2">
        <!-- <button class="font-bold" onclick="toogle()">Add New</button> -->
    </div>
        <!-- section 2 -->
        <div class=" flex flex-col gap-7 p-5">
            <h1 class="font-bold border appraisalH1">Commentary/Review</h1>
            <label for="" class="flex flex-col text-sm font-bold">
                Manager Remark: <input type="text" placeholder="Make a Review">
            </label>

            <label for="" class="flex flex-col text-sm font-bold">
                H.R Remark: <input type="text" placeholder="Make a Review">
            </label>

            <label for="" class="flex flex-col text-sm font-bold">
                M.D's Remark: <input type="text" placeholder="Make a Review">
            </label>

            <label for="" class="flex flex-col text-sm font-bold">
                Employee's comment: <input type="text" placeholder="Make a Review">
            </label>
        </div>
    </div>
  </article>
</div>
</body>
<!-- The core Firebase JS SDK is always required and must be listed first -->
<script src="https://www.gstatic.com/firebasejs/7.13.1/firebase-app.js"></script>
<!-- TODO: Add SDKs for Firebase products that you want to use -->
<script src="https://www.gstatic.com/firebasejs/7.13.1/firebase-storage.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/firebase/7.14.1-0/firebase.js"></script>
<script type="module"  src="/Appraisal.js"></script>
<script   src="/Erp.js"></script>
<script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
<script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>
</html>