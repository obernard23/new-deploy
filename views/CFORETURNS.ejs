<!DOCTYPE html>
<html lang="en">
  <%-include('./partials/head.ejs')%>
  <body>
    <nav class="navbar bg-black">
      <div class="container-fluid">
        <span class="navbar-brand" onclick="history.back()" href="/" title="Back">
          <img 
          width="30"
          hegith="30"
          src="data:image/png;base64,">
         </span>
        <ul class="navbar-nav me-auto mb-2 mb-lg-0">
          <li class="nav-item dropdown">
            <h5>
              <a
                class="nav-link breadcrumb-item active"
                aria-current="page"
                href="/"
                role="button"
                data-bs-toggle="dropdown"
                aria-expanded="false"
                style="color: #fff !important"
              >
               Return Marchandise Authorization
              </a>
            </h5>
          </li>
        </ul>
        <%-include('./partials/USER.ejs')%>
        <%-include('./partials/inactive.ejs')%>
      </div>
    </nav>
     <!--bread crum div -->
     <div class="d-flex justify-content-between p-2 m-1 bg-success-subtle ">
        <h5 class="font-bold"></h5>
        <%if(user.role === "Admin"){%>
            <span onclick="location.assign('')" title="Download in Xlsx">
                <img height="40" width="40" src="data:image/png;base64,">
              </span>
        <%}%> 
       </div>
    <div class="h-59 p-1 d-flex flex-column" >
        <%if(returns.length > 0){%>
            <%returns.forEach(bill=>{%>
                <span  class="list-group position-relative mb-2">
                    <div class="list-group-item list-group-item-action d-flex justify-content-between align-items-center" >
                        <small class="position-absolute top-0  start-50 translate-middle badge rounded-pill bg-warning">
                          <span class="badge bg-secondary"><%=bill.status%></span>
                        </small>

                          <div>
                            <h6>
                                <a class="link-offset-2 link-offset-3-hover link-underline link-underline-opacity-0 link-underline-opacity-75-hover" href="/api/v1/<%=bill._id%>/Returns">RMA/<%=bill.billReferenceNo%>/<%=bill.ReturnDate%></a>
                            </h6>
                          </div>
                      </div> 
                    </span>
            <%})%>
        <%}else{%>
        <div class="container">
            <p>Nothing to show</p>
        </div>
        <%}%>
    </div>

     <!-- The Modal -->
     <div id="myModal" class="modal flex items-center flex-col justify-center gap-3 shadow">
        <!-- Modal content -->
        <div class="modal-body modal-content container">
            
        </div>
     </div>

    </body>
</html>